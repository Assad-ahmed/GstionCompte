{% extends 'base.html.twig' %}

{% block body %}
    {{ form_start(form,{'attr': {'novalidate': 'novalidate'}}) }}
    <div class="row">
        <div class="col">{{ form_row(form.ninea) }}</div>
        <div class="col">{{ form_row(form.nom) }}</div>
        <div class="col">{{ form_row(form.responsable) }}</div>
    </div>
    <div class="row">
        <div class="col">{{ form_row(form.adresse) }}</div>
        <div class="col">{{ form_row(form.chifAffaire) }}</div>
        <div class="col">{{ form_row(form.typeEntreprise) }}</div>
    </div>
    <div class="col"> {{ form_rest(form) }}</div>
    {{ form_end(form) }}
{% endblock %}
{% block PageTitre %}
    Editer d'une entreprise
{% endblock %}

{% block title %}
    Editer d'une entreprise
{% endblock %}
{% block javascripts %}
    <script>
        $(document).ready(function() {
            $('.select2').select2();
        });
    </script>
{% endblock %}

